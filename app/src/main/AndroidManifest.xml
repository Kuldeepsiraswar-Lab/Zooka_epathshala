<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kuldeep.zookaepathshala">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DashBoardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClassActivity"
            android:splitName="Class Name"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"
            tools:targetApi="o" />
        <activity android:name=".PreviousYearPaperActivity"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"/>
        <activity android:name=".ImportantQuestionActivity"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"/>
        <activity android:name=".ModalPaperActivity"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"/>
        <activity android:name=".BoardResultActivity"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"/>
        <activity android:name=".NewsUpdateActivity"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"/>
        <activity android:name=".BookListActivity"
            android:parentActivityName=".DashBoardActivity"
            android:theme="@style/Activity"/>
    </application>

</manifest>